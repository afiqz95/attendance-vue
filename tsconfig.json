{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "experimentalDecorators": true,
    "module": "es2015",
    "moduleResolution": "node",
    "target": "es5",
    "sourceMap": true,
    "skipDefaultLibCheck": true,
    "strict": false,
    "noImplicitAny": false,
    "types": ["webpack-env"],
    "noImplicitThis": false,
    "lib": ["es2015", "dom"]
  },
  "exclude": ["bin", "node_modules"]
}
